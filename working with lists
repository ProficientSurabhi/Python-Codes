new_list= list()
my_list= list(input("Enter a string of characters: "))
n= int(input("Enter the number of values in the range: "))
if n>0:
    for i in range(1,n+1):
        for x in my_list:
            new_list.append(x+str(i))
    print(new_list)
else:
    print('Invalid,Please enter a positive integer for range!')
